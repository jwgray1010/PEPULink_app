# Environment Variables Example
# Copy this file to .env and fill in your actual values

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
EXPO_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# LinkLayer Chain Configuration
EXPO_PUBLIC_LINKLAYER_RPC_URL=https://rpc-linklayer-v2-mainnet-0.t.conduit.xyz
EXPO_PUBLIC_LINKLAYER_CHAIN_ID=97741

# Optional: Additional RPC providers for fallback
EXPO_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
EXPO_PUBLIC_INFURA_PROJECT_ID=your_infura_project_id_here

# App Configuration
EXPO_PUBLIC_APP_NAME=LinkLayer
EXPO_PUBLIC_APP_VERSION=1.0.0
